<div>
    <section>
        <div class="content-wrapper">
            <section class="content">
                <div class="row">
                    <div class="col-md-12">
                        <p class="infotitle">添加汇率</p>
                        <form asp-action="CreateRate" asp-controller="System" method="post">
                            <table class="table table-responsive table-stripted">
                                <tbody>
                                    <tr>
                                        <th class="must2">*国家</th>
                                        <td><input type="text" class="form-control" name="Country" /></td>
                                    </tr>
                                    <tr>
                                        <th class="must2">*汇率</th>
                                        <td><input type="text" class="form-control" name="Exchange" /></td>
                                    </tr>
                                    <tr>
                                        <th><a asp-action="Rate" asp-controller="System" class="form-control btn btn-primary">返回汇率列表</a></th>
                                        <th><input type="submit" class="form-control btn btn-primary" value="添加"></th>
                                    </tr>
                                </tbody>
                            </table>

                        </form>
                        <label class="must2">例子</label><br />
                        <label class="must2">
                            国家：美<br />
                            汇率：6.6
                        </label><br />
                        <div style="height:10px;"></div>
                    </div>
                </div>
            </section>
        </div>
    </section>
</div>




