@model IEnumerable<PreOrder>
@{
    Layout = null;
    //用户所有订单列表
}
<tr>
    @foreach (var x in Model)
    {
    <tr>
        <td>@x.Id</td>
        <td>@x.PreOrderNumber</td>
        <td>@x.Country</td>
        <td>@x.ShopName</td>
        <td><a href="@x.GoodsUrl" title="@x.GoodsUrl" target="_blank">查看</a></td>
        <td><a href="#" title="@x.Address">查看</a></td>
        <td>@x.OrderType</td>
        <td>@x.NextOrToday</td>
        <td>@x.GoodsCost</td>
        <td>@x.Freight</td>
        <td>@x.Total</td>
        <td>@x.RMB</td>
        <td>@x.Poundage</td>
        <td>@x.PayTotal</td>
        <td>@x.TrueOrderNumber</td>
        <td><a href="#" title="@x.FeedBackContent" id="fb_text" >复制</a></td>
        <td><a href="#" title="@x.ReviewContent" id="rew_text" >复制</a></td>
        <td>@x.Draw</td>
        <td><a asp-action="EditOrder" asp-controller="Manage" asp-route-id="@x.Id">编辑</a>&nbsp;&nbsp;<a href="#">删除</a> </td>
        <td>@x.Times</td>
    </tr>
    }
<script src="~/js/jQuery-2.1.4.min.js"></script>
<script type="text/javascript">
    

</script>

