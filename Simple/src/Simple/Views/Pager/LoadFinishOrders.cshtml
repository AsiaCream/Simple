@model IEnumerable<PreOrder>
@{
    Layout = null;
    //管理员查看已完成订单列表
}
@foreach (var x in Model)
{
    <tr>
        <td>@x.Id</td>
        <td>@x.PreOrderNumber</td>
        <td>@x.Country</td>
        <td>@x.ShopName</td>
        <td><a href="@x.GoodsUrl" title="@x.GoodsUrl" target="_blank">查看</a></td>
        <td>@x.FindType</td>
        <td>@x.NextOrToday</td>
        <td>@x.GoodsCost</td>
        <td>@x.Freight</td>
        <td>@x.Rate</td>
        <td>@x.Total</td>
        <td>@x.RMB</td>
        <td>@x.Poundage</td>
        <td>@x.PayTotal</td>
        <td>@x.PostTime</td>
        <td>@x.DrawTime</td>
        <td>@x.FinishTime</td>
        <td>@x.PlatType</td>
        <td>@x.Times</td>
        <td><a asp-action="OrderDetails" asp-controller="Admin" asp-route-id="@x.Id" class="btn btn-primary">详细</a></td>
    </tr>
}


